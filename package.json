{
  "name": "vue-stylefmt",
  "main": "lib/vue-stylefmt.js",
  "version": "0.2.2",
  "description": "Apply stylefmt for vue component",
  "license": "MIT",
  "repository": "https://github.com/tigberd/atom-vue-stylefmt",
  "author": {
    "name": "tigberd",
    "email": "tigberd@gmail.com",
    "url": "https://gisthub.com/tigberd"
  },
  "activationCommands": {
    "atom-workspace": "vue-stylefmt:format"
  },
  "configSchema": {
    "formatOnSave": {
      "title": "Format on Save",
      "description": "Execute formatting Vue component style section on save.",
      "type": "boolean",
      "default": false
    }
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "keywords": [
    "css",
    "format",
    "style",
    "stylefmt",
    "vue",
    "vuejs"
  ],
  "bugs": {
    "url": "https://github.com/tigberd/vue-stylefmt/issues"
  },
  "homepage": "https://github.com/tigberd/vue-stylefmt#readme",
  "dependencies": {
    "postcss": "^5.2.17",
    "postcss-scss": "^0.4.1",
    "stylefmt": "^5.3.2"
  }
}
